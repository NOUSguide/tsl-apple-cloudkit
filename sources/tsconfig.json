{
	"compilerOptions": {
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": true,
		"removeComments": true,
		"strictNullChecks": true,
		"traceResolution": false,
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "../release/tsl-apple-cloudkit/",
		"rootDir": "./",
		"target": "es5",
		"baseUrl": "./",
		"paths": {
			"*": [
				"../node_modules/@types/node/",
				"../node_modules/typescript/"
			]
		}
	},
	"exclude": [
		"../node_modules/",
		"../release/",
		"../tests/"
	],
	"files": [
		"./client/tsl-apple-cloudkit.ts",
		"./install.ts"
	]
}